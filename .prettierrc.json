{
    "printWidth": 160,
    "endOfLine": "auto",
    "useTabs": false,
    "tabWidth": 4,
    "vueIndentScriptAndStyle": true,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "proseWrap": "always",
    "overrides": [
        {
            "files": ["*.vue"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
